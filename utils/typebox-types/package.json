{
  "name": "@fieldzoo/typebox-types",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/cjs/index.js",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc && tsc -p tsconfig.cjs.json && echo '{}' > ./dist/cjs/package.json",
    "clean": "rm -rf node_modules && rm -rf dist",
    "dev": "tsc -w",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-presets/node-esm"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.28.14",
    "ajv-formats": "^2.1.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@types/jest": "^29.5.2",
    "@types/node": "20.1.3",
    "eslint": "^8.40.0",
    "eslint-config-custom": "workspace:*",
    "jest": "^29.5.0",
    "jest-presets": "workspace:*",
    "ts-jest": "^29.1.0",
    "tsconfig": "workspace:*",
    "typescript": "5.1.3"
  }
}
