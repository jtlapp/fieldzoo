{
  "name": "ui-components",
  "version": "0.0.0",
  "description": "Svelte styles and components",
  "license": "MIT",
  "type": "module",
  "module": "./src/index.ts",
  "svelte": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "module": "./src/index.ts",
      "svelte": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./styles/**/*": "./src/styles/*"
  },
  "scripts": {
    "lint": "prettier --check --ignore-path=../../.prettierignore . && eslint \".\"",
    "format": "prettier --write --ignore-path=../../.prettierignore ."
  },
  "devDependencies": {
    "eslint-config-custom-svelte": "workspace:*",
    "svelte": "^4.0.0",
    "tailwind-config": "workspace:*",
    "tsconfig": "workspace:*"
  },
  "dependencies": {
    "@melt-ui/svelte": "^0.22.2"
  }
}
